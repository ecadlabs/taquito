{
  "name": "@taquito/remote-signer",
  "version": "11.1.0",
  "description": "Remote signer provider",
  "keywords": [
    "tezos",
    "blockchain"
  ],
  "main": "dist/taquito-remote-signer.umd.js",
  "module": "dist/taquito-remote-signer.es5.js",
  "typings": "dist/types/taquito-remote-signer.d.ts",
  "files": [
    "signature.json",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Simon Boissonneault-Robert <simon@ecadlabs.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "version-stamp": "node ../taquito/version-stamping.js",
    "build": "npm run version-stamp && tsc --project ./tsconfig.prod.json --module commonjs && rollup -c rollup.config.ts ",
    "start": "npm run version-stamp && rollup -c rollup.config.ts -w"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  },
  "dependencies": {
    "@taquito/http-utils": "^11.1.0",
    "@taquito/taquito": "^11.1.0",
    "@taquito/utils": "^11.1.0",
    "@types/jest": "^26.0.24",
    "libsodium-wrappers": "0.7.8",
    "typedarray-to-buffer": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/libsodium-wrappers": "0.7.8",
    "@types/node": "^17.0.0",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "colors": "^1.4.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "lint-staged": "^12.1.2",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.5.1",
    "prompt": "^1.2.0",
    "replace-in-file": "^6.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.4.0",
    "typescript": "~4.1.5"
  },
  "gitHead": "551e35aeff7d6dcde1c72284238c0ed3c3aae77e"
}
