/* This proxy contract transfers the received amount to the contract given as parameter.
   It is used to test the SOURCE and SENDER opcodes; see source.tz and sender.tz. */
parameter (contract unit) ;
storage unit ;
code{
      UNPAIR;
      AMOUNT ;
      UNIT ;
      TRANSFER_TOKENS;
      DIP {NIL operation} ;
      CONS;
      PAIR
    }