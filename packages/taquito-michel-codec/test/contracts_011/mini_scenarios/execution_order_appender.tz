# Given a storage (adr, str), calls the contract at adr with the
# parameter str
parameter unit;
storage (pair address string);
code {
       CDR;
       DUP;
       UNPAIR;
       CONTRACT string;
       ASSERT_SOME;
       PUSH mutez 0;
       DIG 2;
       TRANSFER_TOKENS;
       NIL operation;
       SWAP;
       CONS;
       PAIR };
