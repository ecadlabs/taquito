# This contract calls the view `id` on the address passed as
# parameter. After returning from the view it stores its BALANCE.
parameter address ;
storage mutez;
code
     { CAR ;
       DUP ;
       PUSH nat 0 ;
       VIEW "id" (pair nat nat) ;
       ASSERT_SOME ;
       DROP ;
       CONTRACT nat ;
       ASSERT_SOME ;
       PUSH mutez 15000000 ;
       PUSH nat 0 ;
       TRANSFER_TOKENS ;
       BALANCE;
       SWAP ;
       NIL operation ;
       SWAP ;
       CONS ;
       PAIR
     } ;


