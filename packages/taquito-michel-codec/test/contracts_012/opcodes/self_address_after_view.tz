# This contract calls the view `id` on the address passed as
# parameter. After returning from the view it stores its
# SELF_ADDRESS.
parameter address ;
storage address ;
code
     { CAR ;
       DUP ;
       PUSH nat 0 ;
       VIEW "id" (pair nat nat) ;
       ASSERT_SOME ;
       DROP ;
       CONTRACT nat ;
       ASSERT_SOME ;
       PUSH mutez 1500 ;
       PUSH nat 0 ;
       TRANSFER_TOKENS ;
       SELF_ADDRESS ;
       SWAP ;
       NIL operation ;
       SWAP ;
       CONS ;
       PAIR
     } ;
