# This contract calls the non-existent view on the address passed as
# parameter. After returning from the view it stores it's
# SELF_ADDRESS.
parameter address ;
storage address ;
code
     { CAR ;
       DUP ;
       PUSH nat 0 ;
       VIEW "id" string ;
       ASSERT_NONE ;
       CONTRACT nat ;
       ASSERT_SOME ;
       PUSH mutez 1500 ;
       PUSH nat 0 ;
       TRANSFER_TOKENS ;
       SELF_ADDRESS ;
       SWAP ;
       NIL operation ;
       SWAP ;
       CONS ;
       PAIR
     } ;



