# This contract calls the noneexistent view on the address passed as
# parameter. After returning from the view it stores it's BALANCE.
parameter address ;
storage mutez;
code
     { CAR ;
       DUP ;
       PUSH nat 0 ;
       VIEW "nonexistent" (pair nat nat) ;
       ASSERT_NONE ;
       CONTRACT nat ;
       ASSERT_SOME ;
       PUSH mutez 15000000 ;
       PUSH nat 0 ;
       TRANSFER_TOKENS ;
       BALANCE;
       SWAP ;
       NIL operation ;
       SWAP ;
       CONS ;
       PAIR
     } ;
